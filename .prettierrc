{
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "parser": "flow",
  "semi": true,
  "useTabs": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "endOfLine": "lf"
}
